!(defprotocol my-protocol (hash pair)
  (cons
    (if (= (+ (car pair) (cdr pair)) 30)
      (cons (cons (cons 'open (cons hash nil)) (empty-env)) pair)
      nil)
    (lambda () (> (car pair) 10)))
  :description "hash opens to a pair (a, b) s.t. a+b=30 and a>10")

